#Program to check whether the string ends with specified value

s="python"
res=s.endswith("N")
print("endswith()")
print(res)

#OUTPUT

endswith()
False

s="python"
res=s.endswith("n")
print("endswith()")
print(res)

#OUTPUT

endswith()
True

s="python"
res=s.endswith("o",0,6)
print("endswith()")
print(res)

#OUTPUT

endswith()
False

s="python"
res=s.endswith("n",0,6)
print("endswith()")
print(res)

#OUTPUT

endswith()
True
